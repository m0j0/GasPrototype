<UserControl x:Class="Prototype.Views.Pipes.Valve3WayExampleView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Prototype.Views.Pipes"
             xmlns:btsr="clr-namespace:Prototype"
             mc:Ignorable="d" d:DataContext="{x:Static btsr:DesignBootstrapper.Valve3WayExampleVm}"
             d:DesignHeight="300" d:DesignWidth="300">
    <PipeFlowCanvas>
        <Valve3Way Canvas.Left="20" Canvas.Top="38" />
        <Valve3Way Canvas.Left="120" Canvas.Top="38" Rotation="Rotate90" />
        <Valve3Way Canvas.Left="220" Canvas.Top="38" Rotation="Rotate180" />
        <Valve3Way Canvas.Left="320" Canvas.Top="38" Rotation="Rotate270" />

    </PipeFlowCanvas>
</UserControl>


        <Pipe Canvas.Left="420" Canvas.Top="76" Orientation="Vertical" Height="50"
              PipeSchemeEx.StartVertex="{Binding ValveFinal4Vertex}" PipeSchemeEx.EndVertex="{Binding Connection_V4_Vertex}" />

<Application x:Class="Prototype.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Prototype"
             xmlns:converters="clr-namespace:Prototype.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Prototype.Core.View;component/Themes/Common.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            <local:DesignBootstrapper x:Key="DesignBootstrapper" />
            <ViewModelToViewConverter x:Key="ViewModelToViewConverter" />
            <converters:AccessConverter x:Key="AccessConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
